<?xml version="1.0" encoding="UTF-8"?>
<app devMode="true"
        interval="10">
    <gateway name="myIOTGateway"
            host="192.168.1.100"
            port="5555">

        <spec max_nodes="64"
              max_channels_per_node="32"/>

        <nodes>
            <node nodeName="zigbeeNode1" nodeAddr="0x40"/>
            <node nodeName="zigbeeNode2" nodeAddr="0x45"/>
        </nodes>

    </gateway>
</app>
